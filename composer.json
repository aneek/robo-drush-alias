{
  "name": "aneek/robo-drush-alias",
  "description": "A Robo task runner for the Acquia Cloud (or Site factory) to download all platform/stack specific aliases.",
  "type": "robo-tasks",
  "keywords": [
    "drupal",
    "blt",
    "robo",
    "deprecated",
    "drush alias"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Aneek Mukhopadhyay",
      "email": "dream.aneek@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "consolidation/robo": "^4.0",
    "typhonius/acquia-php-sdk-v2": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Aneek\\Robo\\DrushAlias\\": "src/"
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^6.4"
  }
}
